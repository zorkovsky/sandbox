    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}/azure/HttpExample/index.js",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "name": "Attach to Node Functions",
            "type": "node",
            "request": "attach",
            "port": 9229,
            "preLaunchTask": "func: host start"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Joi Playground",
            "program": "${workspaceFolder}/joi.js",
        },
        {
            "type": "node",
            "request": "launch",
            "name": "test",
            "program": "${workspaceFolder}/sum.js",
        }
    ]
}